{
  "name": "petux",
  "version": "1.0.0-alpha.10",
  "description": "Emit descriptions of side-effects from your reducers; perform them with a handler of your choice.",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:flow": "mkdir -p lib && cp src/index.js lib/index.js.flow",
    "build": "npm run build:commonjs && npm run build:flow",
    "test": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tempname11/petux.git"
  },
  "keywords": [
    "redux",
    "effects",
    "2017"
  ],
  "author": "Vladimir Semyonov <tempname011@gmail.com> (https://github.com/tempname11)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tempname11/petux/issues"
  },
  "homepage": "https://github.com/tempname11/petux#readme",
  "dependencies": {
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "cross-env": "^3.1.3",
    "flow-bin": "^0.37.4"
  }
}
